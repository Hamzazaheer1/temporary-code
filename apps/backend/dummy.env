# Server Configuration
PORT=4000
NODE_ENV=development

# MongoDB Configuration
# For local MongoDB: mongodb://localhost:27017/monique-powell
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/monique-powell
MONGODB_URI=mongodb://localhost:27017/monique-powell

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=7d

# Session Configuration
SESSION_SECRET=your-session-secret-key-change-this-in-production

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Privy Configuration
# Get these from: https://dashboard.privy.io/
PRIVY_APP_ID=your-privy-app-id
PRIVY_APP_SECRET=your-privy-app-secret

# Google OAuth Configuration (Optional - if still using Google OAuth)
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:4000/api/auth/google/callback

